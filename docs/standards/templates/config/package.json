  "version": "0.0.0",
  "type": "module",
  
  "scripts": {
    "dev": "# プロジェクトタイプに応じて設定",
    "build": "# プロジェクトタイプに応じて設定",
    "start": "# プロジェクトタイプに応じて設定",
    "preview": "# プロジェクトタイプに応じて設定",
    
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest --coverage",
    "test:watch": "vitest --watch",
    
    "clean": "rm -rf dist build .next .nuxt coverage",
    "reset": "rm -rf node_modules package-lock.json && npm install",
    
    "quality": "npm run typecheck && npm run lint && npm run test:run && npm run build",
    "pre-commit": "npm run typecheck && npm run lint && npm run test:run"
  },
  
  "dependencies": {
    "# プロジェクトタイプに応じて追加": "# React/Express/Fastify等"
